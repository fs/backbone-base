    "coffee-script": "~1.6.3",
    "coffeeify": "~0.7.0",
    "connect-history-api-fallback": "0.0.5",
    "connect-prism": "~0.7.5",
    "del": "~0.1.3",
    "gulp": "~3.8.8",
    "gulp-coffeelint": "~0.4.0",
    "gulp-concat": "~2.4.1",
    "gulp-cssimport": "~1.2.3",
    "gulp-ignore": "~1.2.1",
    "gulp-install": "~0.2.0",
    "gulp-jade": "~0.8.0",
    "gulp-jsonlint": "~0.1.0",
    "gulp-notify": "~2.0.0",
    "gulp-plumber": "~0.6.6",
    "gulp-replace-task": "^0.1.0",
    "gulp-stylus": "~1.3.3",
    "jadeify": "3.0.0",
    "jeet": "^6.1.2",
    "karma": "~0.12.16",
    "karma-browserify": "^1.0.0",
    "karma-chai": "~0.1.0",
    "karma-chrome-launcher": "~0.1.4",
    "karma-coffee-preprocessor": "~0.2.1",
    "karma-mocha": "~0.1.4",
    "karma-phantomjs-launcher": "~0.1.4",
    "karma-sinon": "~1.0.3",
    "remapify": "^1.3.0",
    "require-dir": "~0.1.0",
    "run-sequence": "~1.0.1",
    "vinyl-source-stream": "~1.0.0",
    "watchify": "~2.1.1"
  },
  "browser": {
    "jquery": "./bower_components/jquery/dist/jquery.js",
    "underscore": "./bower_components/underscore/underscore.js",
    "backbone": "./bower_components/backbone/backbone.js",
    "backbone-routefilter": "./bower_components/backbone-route-filter/backbone-route-filter.js",
    "backbone-stickit": "./bower_components/backbone.stickit/backbone.stickit.js",
    "backbone-validation": "./bower_components/backbone-validation/dist/backbone-validation.js",
    "marionette": "./bower_components/marionette/lib/backbone.marionette.js",
    "bootstrap": "./bower_components/bootstrap/dist/js/bootstrap.js"
  },
  "browserify-shim": {
    "jquery": "$",
    "underscore": "_",
    "backbone": {
      "exports": "Backbone",
      "depends": [
        "jquery",
        "underscore"
      ]
    },
    "backbone-routefilter": {
      "exports": "Backbone",
      "depends": [
        "backbone"
      ]
    },
    "backbone-stickit": {
      "exports": "Backbone",
      "depends": [
        "backbone"
      ]
    },
    "backbone-validation": {
      "exports": "Backbone",
      "depends": [
        "backbone"
      ]
    },
    "marionette": {
      "exports": "Marionette",
      "depends": [
        "backbone"
      ]
    },
    "bootstrap": {
      "depends": [
        "jquery"
      ]
    },
    "templates": {
      "depends": [
        "jade"
      ]
    }
  }
}
